{
  "author": "christopher.j.wallis@gmail.com",
  "devDependencies": {
    "@types/node": "10.12.18",
    "copyfiles": "2.1.0",
    "cypress": "3.1.5",
    "del-cli": "1.1.0",
    "prettier": "1.16.4",
    "rollup": "0.68.2",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript": "1.0.0",
    "rollup-plugin-uglify": "6.0.2",
    "rollup-watch": "4.3.1",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "typescript": "3.3.3",
    "uglify-js": "3.4.9"
  },
  "homepage": "http://just-animate.com",
  "license": "MIT",
  "main": "./dist/main.js",
  "module": "./dist/module.js",
  "name": "just-animate",
  "repository": {
    "type": "git",
    "url": "https://github.com/just-animate/just-animate"
  },
  "scripts": {
    "build": "npm run copy:types && npm run build:prod && npm run build:dev",
    "build:debug": "rollup -w -c ./config/rollup.umd.js --configDebug",
    "build:dev": "rollup -c ./config/rollup.umd.js --configDebug",
    "build:prod": "rollup -c ./config/rollup.umd.js",
    "copy:types": "copyfiles -f ./src/types.d.ts dist",
    "clean": "node_modules/.bin/del-cli -f dist",
    "format": "prettier --print-width 80 --single-quote --list-different --write \"src/**/*.ts\"",
    "lint": "tslint --project .",
    "start": "npm run clean && npm run lint && npm run build",
    "test": "npm run build:dev && ./node_modules/.bin/cypress run",
    "test:debug": "npm run build:debug & ./node_modules/.bin/cypress open"
  },
  "types": "./types/main",
  "typings": "./types/main",
  "typeRoots": [
    "node_modules/@types"
  ],
  "version": "3.0.0-alpha-0"
}
